# editorconfig.org
root = true

[*]
indent_size = 2
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[{*.js,*.jsx}]
indent_size = 2

[*.json]
indent_size = 2
root = true

[*.md]
trim_trailing_whitespace = false

[*.yml]
indent_size = 2
